{
    "action": {
        "default_icon": {
            "16": "img/icon16.png"
        }
    },
    "author": "CreatechStudio Shanghai Inc.",
    "background": {
        "service_worker": "background.js"
    },
    "description": "Sends Download Links to Aria Download Manager",
    "homepage_url": "https://github.com/iewnfod/aria-download-manager",
    "host_permissions": [ "\u003Call_urls>" ],
    "icons": {
        "128": "img/icon128.png",
        "16": "img/icon16.png",
        "48": "img/icon48.png"
    },
    "manifest_version": 3,
    "name": "AriaDownloadManager Extension",
    "permissions": [ 
        "downloads"
    ],
    "update_url": "https://github.com/iewnfod/aria-download-manager/extension/chrome/dist",
    "version": "0.0.1",
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    },
    "web_accessible_resources": [ {
        "matches": [ "\u003Call_urls>" ],
        "resources": [ "img/icon16.png", "img/icon16_2x.png", "img/close16.png", "img/close16_2x.png" ]
    } ]
}